<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="g5.elevator.controllers.instruction.InstructionController">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="100.0" minWidth="50.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="timestampLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" style="-fx-border-color: black;" text="timestampLabel" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <HBox alignment="CENTER_RIGHT" style="-fx-border-color: black;" GridPane.columnIndex="1">
         <children>
            <Label fx:id="pickupTimeLabel" text="pickupTimeLabel" />
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" style="-fx-border-color: black;" GridPane.columnIndex="2">
         <children>
            <Label fx:id="dropOffTimeLabel" text="dropOffTimeLabel" />
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" style="-fx-border-color: black;" GridPane.columnIndex="3">
         <children>
            <Label fx:id="totalTimeLabel" text="totalTimeLabel" />
         </children>
      </HBox>
   </children>
</GridPane>
